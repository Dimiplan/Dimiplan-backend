{
  "source": {
    "include": ["./src/", "./index.mjs"],
    "includePattern": "\\.(js|mjs)$",
    "exclude": ["./node_modules/", "./logs/", "./keys/", "./admin/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true
  },
  "plugins": ["plugins/markdown", "jsdoc-route-plugin"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
