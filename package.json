  "description": "",
  "exports": {
    ".": "./index.mjs",
    "./auto-pull": "./src/scripts/autoPull.mjs"
  },
  "scripts": {
    "dev": "bun --watch index.mjs",
    "auto-pull": "bun src/scripts/autoPull.mjs",
    "lint": "eslint src/ index.mjs --ext .mjs,.js",
    "lint:fix": "eslint src/ index.mjs --ext .mjs,.js --fix",
    "docs": "jsdoc -c .jsdoc.config.json",
    "docs:clean": "rm -rf docs/ && npm run docs",
    "docs:json": "jsdoc -c .jsdoc.config.json -X > docs/api-docs.json",
    "docs:serve": "npm run docs:json && node -e \"console.log('JSDoc JSON generated at docs/api-docs.json')\""
  },
