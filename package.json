    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^8.1.0",
    "https": "^1.0.0",
    "mysql": "^2.18.1",
    "nodemon": "^3.1.10",
    "openai": "^5.3.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
