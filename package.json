    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "https": "^1.0.0",
    "mysql": "^2.18.1",
    "nodemon": "^3.1.10",
    "openai": "^5.8.2",
    "passport": "^0.7.0",
